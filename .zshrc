# -------------------------------------------------------------------
# file system alias settings
# -------------------------------------------------------------------
alias la='ls -a'
alias ll='ls -pla'
alias cll='clear && ll'


# -------------------------------------------------------------------
# git alias settings
# -------------------------------------------------------------------
alias gst='cll && git status'
alias glo='cll && git log --oneline'
alias gll='cll && git log'
alias gaa='git add . && gst'
alias gpo='git push origin && gst'
alias gpl='git pull origin && gst'

