# -------------------------------------------------------------------
# file system alias settings
# -------------------------------------------------------------------
alias la='ls -a'
alias ll='ls -pla'
alias cll='clear && ll'


# -------------------------------------------------------------------
# git alias settings
# -------------------------------------------------------------------
alias gst='cll && git status'
alias glo='cll && git log --oneline'
alias gaa='git add . && gst'
alias gpo='git push origin'
alias gpl='git pull origin'

